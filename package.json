{
    "name": "npm-template-node",
    "version": "1.0.0",
    "description": "这是一个可用于发布npm包的node开发工作空间模板。This is a node development workspace template that can be used to publish npm packages.",
    "author": "Yesifang",
    "license": "MIT",
    "private": false,
    "main": "./dist/npm-tempalte-node.js",
    "homepage": "https://yesifang.com",
    "repository": {
        "type": "git",
        "url": "git@github.com:SuperYesifang/npm-template-node.git"
    },
    "keywords": [
        "npm",
        "node",
        "workspace",
        "template",
        "package",
        "development"
    ],
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/preset-env": "^7.16.11",
        "babel-loader": "^8.2.5",
        "html-webpack-plugin": "^5.5.0",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1"
    }
}
